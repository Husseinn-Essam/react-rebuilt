{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
   
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    "no-unused-vars": "warn",
    "quotes":"off",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "no-plusplus" : "off",
    "no-param-reassign":["error", { "props": false }],
    "no-use-before-define":["error",{"func":false}]
    }
} 
